<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE 
hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"   
	     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.h3c.service.base.models.UserSign" table="usersign" lazy="true">
		<id name="id" column="Id" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="userId" column="UserId" type="int" />
		<property name="personalSign" column="PersonalSign" type="string" />
	</class>
</hibernate-mapping>